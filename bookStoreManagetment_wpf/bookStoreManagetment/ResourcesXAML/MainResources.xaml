    <viewmodel:KhachtrahangViewModel x:Key="KhachtrahangVM"></viewmodel:KhachtrahangViewModel>

    <viewmodel:DSHoaDonViewModel x:Key="DSHoaDonVM"></viewmodel:DSHoaDonViewModel>

    <viewmodel:DSNhanVienViewModel x:Key="DSNhanVienVM" ></viewmodel:DSNhanVienViewModel>
    <viewmodel:DSKhachHangViewModel x:Key="DSKhachHangVM" ></viewmodel:DSKhachHangViewModel>
    <viewmodel:ThongTinNhanVienViewModel x:Key="ThongTinNhanVienVM" ></viewmodel:ThongTinNhanVienViewModel>

    <DataTemplate DataType="{x:Type viewmodel:NhacungcapViewMode}">
        <view:NhacungcapUC></view:NhacungcapUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:AddSupplierViewModel}">
        <view:AddSupplier></view:AddSupplier>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:EditSupplierViewModel}">
        <view:EditSupplier></view:EditSupplier>
    </DataTemplate>
</ResourceDictionary>
    